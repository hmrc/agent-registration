# microservice specific routes

GET         /application             uk.gov.hmrc.agentregistration.controllers.ApplicationController.findApplication
GET         /application/linkId/:linkId     uk.gov.hmrc.agentregistration.controllers.ApplicationController.findApplicationByLinkId(linkId: LinkId)
GET         /application/by-agent-applicationId/:agentApplicationId  uk.gov.hmrc.agentregistration.controllers.ApplicationController.findApplicationById(agentApplicationId: AgentApplicationId)
POST        /application             uk.gov.hmrc.agentregistration.controllers.ApplicationController.upsertApplication

GET         /business-partner-record/utr/:utr   uk.gov.hmrc.agentregistration.controllers.BusinessPartnerRecordController.getBusinessPartnerRecord(utr: Utr)

GET         /member-provided-details/by-agent-applicationId/:agentApplicationId     uk.gov.hmrc.agentregistration.controllers.providedetails.llp.MemberProvidedDetailsController.findByAgentApplicationId(agentApplicationId: AgentApplicationId)
GET         /member-provided-details                                                uk.gov.hmrc.agentregistration.controllers.providedetails.llp.MemberProvidedDetailsController.findAll
POST        /member-provided-details uk.gov.hmrc.agentregistration.controllers.providedetails.llp.MemberProvidedDetailsController.upsert

