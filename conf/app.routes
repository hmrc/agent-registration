# microservice specific routes

GET         /application             uk.gov.hmrc.agentregistration.controllers.ApplicationController.findApplication
GET         /application/linkId/:linkId     uk.gov.hmrc.agentregistration.controllers.ApplicationController.findApplicationByLinkId(linkId: LinkId)
POST        /application             uk.gov.hmrc.agentregistration.controllers.ApplicationController.upsertApplication

GET         /business-partner-record/utr/:utr   uk.gov.hmrc.agentregistration.controllers.BusinessPartnerRecordController.getBusinessPartnerRecord(utr: Utr)

GET         /member-provided-details/agent-applicationId/:agentApplicationId              uk.gov.hmrc.agentregistration.controllers.providedetails.llp.MemberProvidedDetailsController.findMemberProvidedDetailsByApplicationId(agentApplicationId: AgentApplicationId)
POST        /member-provided-details uk.gov.hmrc.agentregistration.controllers.providedetails.llp.MemberProvidedDetailsController.upsertMemberProvidedDetails

